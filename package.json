{
  "name": "mikro-orm-playground",
  "version": "1.0.0",
  "description": "Mikro-ORM playground",
  "main": "./server/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prestart": "npm run build",
    "start": "node dist/framework/start",
    "start:watch": "tsnd src/framework/start",
    "start:debug": "tsnd --inspect -- src/framework/start",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malizeus/mikro-orm-playground.git"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.9.0",
    "@mikro-orm/cli": "^4.5.9",
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/migrations": "^4.5.9",
    "@mikro-orm/postgresql": "^4.5.9",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.1",
    "auto-bind": "^4.0.0",
    "bluebird": "^3.7.2",
    "bottlejs": "^2.0.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "method-override": "^3.0.0",
    "ts-node": "^10.2.1",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.2",
    "utility-types": "^3.10.0"
  },
  "author": "malizeus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/malizeus/mikro-orm-playground/issues"
  },
  "homepage": "https://github.com/malizeus/mikro-orm-playground#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./app/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
